#!/bin/bash

export COMPOSE_PROJECT_NAME="$ORG_NAME-$PROJECT_NAME-$ENV_NAME"
export COMPOSE_FILE="docker-compose.yml:docker-compose.db.yml:docker-compose.dev.yml"
